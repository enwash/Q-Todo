{
  "name": "Q Todo",
  "description": "Beautiful new tab page to replace the default",
  "version": "1.0.2",
  "permissions": [
    "declarativeContent",
    "storage",
    "http://fonts.googleapis.com/",
    "https://fonts.googleapis.com/"
  ],
  "background": {
    "page": "bg.html"
  },
  "incognito": "split",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "Q Todo"
  },
  "manifest_version": 2,
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png" }
}